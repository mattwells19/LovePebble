{
  "env": {
    "browser": true
  },
  "extends": ["plugin:react/recommended", "../.eslintrc"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "plugins": ["react"],
  "rules": {
    "react/prop-types": "off",
    "react/display-name": "off"
  }
}
